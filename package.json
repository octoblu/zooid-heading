{
  "name": "zooid-heading",
  "version": "1.0.10",
  "description": "Heading",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint src stories",
    "lint:js:fix": "eslint src stories --fix",
    "build": "webpack",
    "test": "mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "coverage": "nyc npm test",
    "mocha:json": "mocha --reporter json > coverage/mocha.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-heading.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-heading/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-heading#readme",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chai-enzyme": "^0.8.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^11.1.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.2",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "emotion": "^7.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1"
  }
}
